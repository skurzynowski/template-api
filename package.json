{
  "name": "converter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"integration-tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start:dev -- --debug 0.0.0.0:9229",
    "start:prod": "node dist/main",
    "lint": "eslint ./{src,integration-tests}/**/*.ts --max-warnings=0",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:cov": "npm run test:unit -- --coverage",
    "test:integration": "cross-env NODE_ENV=test jest --config jest-integration.config.js --runInBand --forceExit",
    "test:integration:cov": "npm run test:integration -- --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "node dist/scripts/scripts.js run",
    "migration:revert": "node dist/scripts/scripts.js revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/pizzip": "^3.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "docxtemplater": "^3.19.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "faker": "^5.1.0",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "joi": "^17.3.0",
    "pg": "^8.5.1",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "pizzip": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.5",
    "tslib": "^2.0.3",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.5",
    "@types/fs-extra": "^9.0.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.16",
    "@types/joi": "^14.3.4",
    "@types/node": "^14.14.10",
    "@types/pino": "^6.3.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
